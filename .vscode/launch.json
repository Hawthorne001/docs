{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cobol",
      "request": "launch",
      "name": "COBOL (native)",
      "program": "${workspaceFolder}/<insert-program-name-here>",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": true
    },

    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
    },
  ]
}